<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="CustomTextViewStyle" parent="android:Widget.TextView">
        <item name="android:textColor">@color/semi_dark</item>
        <item name="android:textSize">20sp</item>
    </style>

    <style name="ListingTextViewStyle" parent="android:Widget.TextView">
        <item name="android:textColor">@color/dark</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="CustomButtonStyle" parent="Widget.AppCompat.Button">
        <item name="android:textColor">@color/dark</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:background"> @drawable/button_default</item>
    </style>
    <style name="CustomButtonStyle2" parent="Widget.AppCompat.Button">
        <item name="android:textColor">@color/dark</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:background"> @drawable/button_rectangle</item>
    </style>
</resources>